{
  "name": "node-archive",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "mocha \"**/*.test.ts\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm test && pnpm run format:check && pnpm lint && pnpm build"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
  },
  "devDependencies": {
    "@types/chai": "^4.3.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shahradelahi/node-archive.git"
  },
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/shahradelahi/node-archive/issues"
  },
  "homepage": "https://github.com/shahradelahi/node-archive#readme"
}
